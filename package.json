{
  "name": "cube",
  "version": "0.0.0",
  "author": "chick307 <chick307@gmail.com>",
  "build": {
    "appId": "com.chick307.app.cube",
    "files": [
      "dist/webpack/prod/**/*.*"
    ],
    "mac": {
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeExtensions": [
              "*"
            ],
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Alternate"
          }
        ]
      },
      "target": [
        "zip"
      ]
    },
    "productName": "Cube"
  },
  "dependencies": {
    "dompurify": "^2.3.0",
    "electron-store": "^8.0.0",
    "highlight.js": "^10.7.2",
    "jsdom": "^17.0.0",
    "jszip": "^3.6.0",
    "marked": "^2.1.3",
    "pdfjs-dist": "^2.7.570",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/dompurify": "^2.2.3",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.13",
    "@types/marked": "^2.0.4",
    "@types/pdfjs-dist": "^2.7.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "electron": "^12.0.7",
    "electron-builder": "^22.11.2",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.5.1",
    "postcss": "^8.2.14",
    "postcss-selector-parser": "^6.0.5",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0"
  },
  "keywords": [],
  "license": "MIT",
  "main": "dist/webpack/prod/entries/main.js",
  "private": true,
  "scripts": {
    "build:dev": "npm run build:dev:webpack",
    "build:dev:webpack": "webpack --config webpack.dev.js",
    "build:prod:mac": "npm run build:prod:webpack && electron-builder --mac --x64",
    "build:prod:webpack": "webpack --config webpack.prod.js",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "start:dev": "electron dist/webpack/dev/entries/main.js",
    "test": "jest",
    "watch": "webpack --config webpack.dev.js --watch"
  }
}
