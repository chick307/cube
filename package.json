{
  "name": "cube",
  "version": "0.0.0",
  "author": "chick307 <chick307@gmail.com>",
  "build": {
    "appId": "com.chick307.app.cube",
    "files": [
      "dist/webpack/prod/**/*.*"
    ],
    "mac": {
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeExtensions": [
              "*"
            ],
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Alternate"
          }
        ]
      },
      "target": [
        "zip"
      ]
    },
    "productName": "Cube"
  },
  "dependencies": {
    "electron-store": "^8.0.0",
    "highlight.js": "^11.4.0",
    "jszip": "^3.9.1",
    "katex": "^0.15.3",
    "lowlight": "^2.6.1",
    "pdfjs-dist": "^2.13.216",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-react": "^7.0.4",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/hast": "^2.3.4",
    "@types/jest": "^27.4.1",
    "@types/pdfjs-dist": "^2.7.4",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "electron": "^18.0.3",
    "electron-builder": "^22.14.13",
    "eslint": "^8.12.0",
    "eslint-plugin-react": "^7.29.4",
    "hast-util-to-text": "^3.1.1",
    "hastscript": "^7.0.2",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-nesting": "^10.1.4",
    "postcss-selector-parser": "^6.0.10",
    "postcss-url": "^10.1.3",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "license": "MIT",
  "main": "dist/webpack/prod/entries/main.cjs",
  "private": true,
  "scripts": {
    "build:dev": "npm run build:dev:webpack",
    "build:dev:webpack": "webpack --config webpack.dev.js",
    "build:prod:mac:arm64": "npm run build:prod:webpack && electron-builder --mac --arm64",
    "build:prod:mac:x64": "npm run build:prod:webpack && electron-builder --mac --x64",
    "build:prod:webpack": "webpack --config webpack.prod.js",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules jest --collect-coverage",
    "lint": "eslint --ext .cjs,.js,.ts,.tsx .",
    "start:dev": "electron dist/webpack/dev/entries/main.cjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "webpack --config webpack.dev.js --watch"
  },
  "type": "module"
}
