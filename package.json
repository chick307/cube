{
  "name": "cube",
  "version": "0.0.0",
  "author": "chick307 <chick307@gmail.com>",
  "build": {
    "appId": "com.chick307.app.cube",
    "files": [
      "dist/webpack/prod/**/*.*"
    ],
    "mac": {
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeExtensions": [
              "*"
            ],
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Alternate"
          }
        ]
      },
      "target": [
        "zip"
      ]
    },
    "productName": "Cube"
  },
  "dependencies": {
    "dompurify": "^2.3.1",
    "electron-store": "^8.0.0",
    "highlight.js": "^11.2.0",
    "jsdom": "^18.0.1",
    "jszip": "^3.7.1",
    "marked": "^3.0.3",
    "pdfjs-dist": "^2.9.359",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/dompurify": "^2.2.3",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/marked": "^3.0.0",
    "@types/pdfjs-dist": "^2.7.4",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.11.7",
    "eslint": "^8.1.0",
    "eslint-plugin-react": "^7.25.1",
    "jest": "^27.1.1",
    "mini-css-extract-plugin": "^2.2.2",
    "postcss": "^8.3.6",
    "postcss-selector-parser": "^6.0.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "keywords": [],
  "license": "MIT",
  "main": "dist/webpack/prod/entries/main.js",
  "private": true,
  "scripts": {
    "build:dev": "npm run build:dev:webpack",
    "build:dev:webpack": "webpack --config webpack.dev.js",
    "build:prod:mac:x64": "npm run build:prod:webpack && electron-builder --mac --x64",
    "build:prod:mac:arm64": "npm run build:prod:webpack && electron-builder --mac --arm64",
    "build:prod:webpack": "webpack --config webpack.prod.js",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "start:dev": "electron dist/webpack/dev/entries/main.js",
    "test": "jest",
    "watch": "webpack --config webpack.dev.js --watch"
  }
}
