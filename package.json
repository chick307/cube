{
  "name": "cube",
  "version": "0.0.0",
  "author": "chick307 <chick307@gmail.com>",
  "build": {
    "appId": "com.chick307.app.cube",
    "files": [
      "dist/webpack/prod/**/*.*"
    ],
    "mac": {
      "extendInfo": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeExtensions": [
              "*"
            ],
            "CFBundleTypeRole": "Viewer",
            "LSHandlerRank": "Alternate"
          }
        ]
      },
      "target": [
        "zip"
      ]
    },
    "productName": "Cube"
  },
  "dependencies": {
    "dompurify": "^2.3.6",
    "electron-store": "^8.0.0",
    "highlight.js": "^11.4.0",
    "jsdom": "^19.0.0",
    "jszip": "^3.7.1",
    "marked": "^4.0.12",
    "pdfjs-dist": "^2.12.313",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/marked": "^4.0.2",
    "@types/pdfjs-dist": "^2.7.4",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss": "^8.4.6",
    "postcss-loader": "^6.2.1",
    "postcss-nesting": "^10.1.2",
    "postcss-selector-parser": "^6.0.9",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "keywords": [],
  "license": "MIT",
  "main": "dist/webpack/prod/entries/main.js",
  "private": true,
  "scripts": {
    "build:dev": "npm run build:dev:webpack",
    "build:dev:webpack": "webpack --config webpack.dev.js",
    "build:prod:mac:x64": "npm run build:prod:webpack && electron-builder --mac --x64",
    "build:prod:mac:arm64": "npm run build:prod:webpack && electron-builder --mac --arm64",
    "build:prod:webpack": "webpack --config webpack.prod.js",
    "coverage": "NODE_OPTIONS=--experimental-vm-modules jest --collect-coverage",
    "lint": "eslint --ext .cjs,.js,.ts,.tsx .",
    "start:dev": "electron dist/webpack/dev/entries/main.cjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "watch": "webpack --config webpack.dev.js --watch"
  },
  "type": "module"
}
